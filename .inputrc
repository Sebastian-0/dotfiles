# Resources for settings terminal escape codes:
# - https://gist.github.com/fnky/458719343aabd01cfb17a3a4f7296797
set editing-mode vi
set show-mode-in-prompt on
set vi-ins-mode-string └\1\e[6 q\e[34m\2[ 󰏪 ] \1\e[0m\2
set vi-cmd-mode-string └\1\e[2 q\e[38;5;216m\2[  ] \1\e[0m\2

set keymap vi-insert
RETURN: "\e\n"
